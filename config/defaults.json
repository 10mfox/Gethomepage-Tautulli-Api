{
  "userFormats": {
    "fields": [
      {
        "id": "status_message",
        "template": "${is_watching} ( ${last_played} )"
      },
      {
        "id": "progress",
        "template": "${progress_time} - ${progress_percent}%"
      },
      {
        "id": "activity",
        "template": "${media_type} - ${friendly_name} - ${total_plays} plays"
      },
      {
        "id": "playback",
        "template": "${stream_container_decision}"
      },
      {
        "id": "last_seen",
        "template": "${last_seen_formatted}"
      }
    ]
  },
  "mediaFormats": {
    "shows": {
      "fields": [
        {
          "id": "title",
          "template": "${grandparent_title} - S${parent_media_index}E${media_index} - ${title}"
        },
        {
          "id": "details",
          "template": "${content_rating} - ${video_resolution}"
        },
        {
          "id": "added",
          "template": "Added ${added_at_relative}"
        }
      ]
    },
    "movies": {
      "fields": [
        {
          "id": "title",
          "template": "${title} (${year})"
        },
        {
          "id": "details",
          "template": "${content_rating} - ${video_resolution}"
        },
        {
          "id": "added",
          "template": "Added ${added_at_relative}"
        }
      ]
    },
    "music": {
      "fields": [
        {
          "id": "title",
          "template": "${parent_title} - ${title}"
        },
        {
          "id": "details",
          "template": "${year} - ${studio} - ${genres}"
        },
        {
          "id": "added",
          "template": "Added ${added_at_relative}"
        }
      ]
    }
  },
  "sections": {
    "movies": [],
    "shows": [],
    "music": []
  }
}